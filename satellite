#include <iostream> /
#include <windows.h>
#include <iomanip>
using namespace std;



struct Sattellite
{

    string type;
    int range;
    int timings;
    int frequency;
    string orbit;
    int distanceorbit;


}p1[2],p2[2],p3[2],p4[2],p5[2],p6[2];
int d1, d2, d3, d4, d5, d6;
int october[6] = {d1,d2,d3,d4,d5,d6};

void observingplanet1()
{
    int temp,max;
    for (int i = 0; i < 2; i++)
    {

        cout << "Enter The Type Of Satellite" << endl;
        cin >> p1[i].type;
        cout << "\nEnter The Range Of Satellite" << endl;
        cin >> p1[i].range;
        cout << "\nEnter The Frequency Of Satellite" << endl;
        cin >> p1[i].frequency;
        cout << "\nEnter The Name  Of Planet Satellite Is Orbiting" << endl;
        cin >> p1[i].orbit;
        cout <<"\nEnter The Distance  Of Satellite From Planet" << endl;
        cin >> p1[i].distanceorbit;

    }
    d1 = p1[0].distanceorbit;
    for (int j = 0; j < 2; j++)
    {
        temp = p1[j].distanceorbit;
        if (temp < d1)
        {
            d1 = temp;
        }
    }
    for (int k = 0; k < 2; k++)
    {
        if (p1[k].distanceorbit == d1)
        {
            cout<< "NEAREST PLANET IS "<<p1[k].type<<endl;
        }
    }
}
